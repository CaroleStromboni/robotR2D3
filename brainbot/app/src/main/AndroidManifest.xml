<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="fr.cesgenslab.brainbot">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_dossier"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".homeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".consulterDossierActivity"/>
        <activity android:name=".informationActivity"/>
        <activity android:name=".consulterDossierWebAppActivity"/>
        <activity android:name=".jeuxWebAppActivity"/>
        <activity android:name=".formulaireWebAppActivity"/>
    </application>

</manifest>